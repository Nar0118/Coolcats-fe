  "scripts": {
    "create-menu": "node src/utils/menu.js",
    "copycontent-public": "copyfiles \"src/content/**\" public --u 1",
    "copycontent-build": "copyfiles \"src/content/**\" build --u 1",
    "copyfiles-public": "copyfiles \"src/games/**/assets/**/*.**\" public --u 3",
    "copyfiles-build": "copyfiles \"src/games/**/assets/**/*.**\" build --u 3",
    "start": "npm run create-menu && npm run copycontent-public && npm run copyfiles-public && env-cmd -f .env.dev react-scripts start",
    "start-prod": "npm run create-menu && npm run copycontent-public && npm run copyfiles-public && env-cmd -f .env.prod react-scripts start",
    "build": "npm run create-menu && npm run copycontent-build && npm run copyfiles-build && env-cmd -f .env.prod react-scripts build",
    "build-dev": "npm run create-menu && npm run copycontent-build && npm run copyfiles-build && env-cmd -f .env.dev react-scripts build"
  },
